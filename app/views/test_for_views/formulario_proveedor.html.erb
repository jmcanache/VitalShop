<div class="container-fluid" id="fp-main-container">
	
	<!--Inicia Div con brackground-->
	<div class="row" id="fp-background"> 
		<%= render 'layouts/header_enlaces' %>
		<!--Inicia Formulario de registo-->
		<section class="col-md-8 col-xs-10 col-md-offset-2 col-xs-offset-1 fp-trans margin-container-fluid">
			<form class="fp-form-div1">
      			<div class="col-xs-12 text-center fp-title-div-1">
      				<h3>CONVIÉRTETE EN PROVEEDOR</h3> 
      				<p>Completa el siguiente formulario</p>
      			</div>
      			<div class="col-sm-6 col-xs-12 fp-border-right">
					<div class="form-group">
					    <label for="fp-descripcion">Descripcíon de su empresa</label>
					    <textarea class="form-control" id="fp-descripcion"></textarea>
					</div>
					<div class="form-group">
					    <label for="fp-nombre">Nombre / Razón social</label>
					    <input type="text" class="form-control" id="fp-nombre">
					</div>
					<div class="form-group">
					    <label for="fp-tipo-documento">Tipo de documento*</label>
					    <input type="text" class="form-control" id="fp-tipo-documento">
					</div>
					<div class="form-group">
					    <label for="fp-numero-documento">Número de documento*</label>
					    <input type="text" class="form-control" id="fp-numero-documento">
					</div>
					<div class="form-group">
					    <label for="fp-nit">Nit</label>
					    <input type="text" class="form-control" id="fp-nit">
					</div>
				</div>
				<div class="col-sm-6 col-xs-12 fp-div1-column2">
					<div class="form-group">
					    <label for="fp-fax">Fax</label>
					    <input type="text" class="form-control" id="fp-fax">
					</div>
					<div class="form-group">
					    <label for="fp-postal">Código postal</label>
					    <input type="text" class="form-control" id="fp-postal">
					</div>
					<div class="form-group">
					    <label for="fp-documento">Documentos legales</label>
					    <input type="text" class="form-control" id="fp-documento">
					</div>
					<div class="form-group">
					    <label for="fp-natural-empresa">Persona natural o empresa</label>
					  	<select class="form-control" id="fp-natural-empresa">
						    <option>Persona natural</option>
						    <option>Empresa</option>
					  	</select>
					</div>
					<h3><span>Sube tus productos</span></h3>
					<button type="submit" class="btn green-btn">Enviar</button>
				</div>
			</form>	
		</section><!--./Fin Formulario de registo-->

		<!--Inicia Subir productos-->
